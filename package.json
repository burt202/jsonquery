{
  "name": "jsonquery",
  "version": "1.11.0",
  "license": "ISC",
  "repository": {
    "type": "https",
    "url": "https://github.com/burt202/jsonquery"
  },
  "scripts": {
    "lint": "eslint src/ --ext .js,.jsx",
    "test": "NODE_ENV='production' mocha --opts test/mocha-unit.opts",
    "start": "webpack-dev-server --progress --colors",
    "build": "rm -rf build; webpack --config ./config/webpack.config.prod.js --progress",
    "deploy": "gh-pages -d build",
    "prepush": "npm run build; npm run deploy"
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ./node_modules/fit-commit-js/lib/hook.js .git/COMMIT_EDITMSG",
      "pre-commit": "npm run lint && npm run test",
      "pre-push": "npm run prepush"
    }
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all",
    "bracketSpacing": false,
    "printWidth": 100
  },
  "dependencies": {
    "chart.js": "^2.9.3",
    "classnames": "^2.2.6",
    "create-react-class": "^15.6.3",
    "date-fns": "^1.29.0",
    "flat": "^5.0.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.12.1",
    "ramda": "^0.26.0",
    "react": "^16.13.1",
    "react-chartjs-2": "^2.9.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.13.1",
    "react-syntax-highlighter": "^9.0.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "uuid": "^7.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^3.5.3",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "fit-commit-js": "^0.3.2",
    "gh-pages": "^2.2.0",
    "husky": "^3.0.0",
    "jsx-loader": "^0.13.2",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^7.1.2",
    "nunjucks": "^3.2.1",
    "nunjucks-webpack-plugin": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.19.1",
    "react-hot-loader": "^4.12.21",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
